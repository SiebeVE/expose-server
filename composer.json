{
    "name": "beyondcode/expose-server",
    "type": "project",
    "description": "Create public URLs for local sites through any firewall and VPN.",
    "keywords": [
        "expose",
        "tunnel",
        "ngrok"
    ],
    "homepage": "https://sharedwithexpose.com",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcel Pociot",
            "email": "marcel@beyondco.de"
        },
        {
            "name": "Diana Scharf",
            "email": "diana@beyondco.de"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-json": "*",
        "beyondcode/expose-common": "dev-main",
        "guzzlehttp/guzzle": "^7.5",
        "guzzlehttp/psr7": "^1.7",
        "illuminate/http": "^10.0",
        "illuminate/log": "^10.0",
        "illuminate/pipeline": "^10.0",
        "illuminate/validation": "^10.0",
        "illuminate/view": "^10.0",
        "laravel-zero/framework": "^10.4",
        "laravel-zero/phar-updater": "^1.3",
        "nunomaduro/termwind": "^1.15.1",
        "nyholm/psr7": "^1.3",
        "symfony/expression-language": "^5.2"
    },
    "require-dev": {
        "clue/block-react": "^1.4",
        "clue/reactphp-sqlite": "dev-modular-worker-for-phar-support",
        "laminas/laminas-http": "^2.13",
        "laravel/pint": "^1.15.1",
        "mockery/mockery": "^1.6.11",
        "phpunit/phpunit": "^10.5.0",
        "react/http": "^1.1.0",
        "react/socket": "^1.6",
        "react/stream": "^1.1.1",
        "twig/twig": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/seankndy/reactphp-sqlite"
        },
        {
            "type": "git",
            "url": "https://github.com/beyondcode/Ratchet"
        },
        {
            "type": "git",
            "url": "https://github.com/beyondcode/expose-common"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["builds/expose-server"]
}
